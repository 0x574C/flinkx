{
  "job": {
    "content": [
      {
        "reader": {
          "name": "mongodboplogreader",
          "parameter": {
            "hostPorts": "127.0.0.1:30001,127.0.0.1:30002,127.0.0.1:30003",
            "username": "root",
            "password": "123456",
            "database": "admin",
            "clusterMode": "REPLICA_SET",
            "authenticationMechanism": "SCRAM-SHA-256",
            "monitorDatabases": ["test"],
            "excludeDocId": false
          }
        },
        "writer": {
          "name": "streamwriter",
          "parameter": {
            "print": true
          }
        }
      }
    ],
    "setting": {
      "speed": {
        "channel": 1,
        "bytes": 1048576
      },
      "errorLimit": {
        "record": 100
      },
      "restore" : {
        "isRestore" : true,
        "isStream" : true
      }
    }
  }
}