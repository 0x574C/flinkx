{
  "job": {
    "content": [
      {
        "writer": {
          "name": "streamwriter",
          "parameter": {
            "print": true
          }
        },
        "reader": {
          "name": "hdfsreader",
          "parameter": {
            "path": "hdfs://ns1/user/hive/warehouse/parquet_tb",
            "defaultFS": "hdfs://ns1",
            "hadoopConfig": {
              "fs.hdfs.impl": "org.apache.hadoop.hdfs.DistributedFileSystem",
              "dfs.ha.namenodes.ns1":"nn1,nn2",
              "dfs.namenode.rpc-address.ns1.nn2":"cdh-impala2:9000",
              "dfs.client.failover.proxy.provider.ns1":"org.apache.hadoop.hdfs.server.namenode.ha.ConfiguredFailoverProxyProvider",
              "dfs.namenode.rpc-address.ns1.nn1":"cdh-impala1:9000",
              "dfs.nameservices":"ns1",
              "hadoop.user.name": "root"
            },
            "column": [
              {
                "name": "id",
                "type": "INT"
              },
              {
                "name": "name",
                "type": "string"
              }
            ],
            "fileType": "parquet",
            "fieldDelimiter": "\u0001",
            "encoding": "utf-8"
          }
        }
      }
    ],
    "setting": {
      "speed": {
        "channel": 1,
        "bytes": 1048576
      },
      "errorLimit": {
        "record": 100
      }
    }
  }
}